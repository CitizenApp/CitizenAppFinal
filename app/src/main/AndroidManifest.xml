<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bluescreen.citizenapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo4"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo4"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Apoderados.Apoderadologin"></activity>
        <activity android:name=".PrincipalApoderado" />
        <activity android:name=".DetallesArchivos.DetallesArchivo" />
        <activity
            android:name=".Administrador.AdministradorPrincipal"
            android:label="@string/title_activity_administrador_principal"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ProfeInicioActivity"
            android:label="@string/title_activity_profe_inicio"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Profe.Registerprueba" />
        <activity android:name=".Profe.Profeactivity" />
        <activity
            android:name=".ui.desafiosTabbedFragment"
            android:label="@string/title_activity_desafios_tabbed_fragment"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".InicioActivity"
            android:label="@string/title_activity_inicio"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
    </application>

</manifest>